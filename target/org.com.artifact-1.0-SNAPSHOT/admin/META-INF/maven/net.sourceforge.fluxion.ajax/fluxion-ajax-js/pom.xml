<?xml version="1.0"?><project>
 <parent>
   <groupId>net.sourceforge.fluxion.tools</groupId>
   <artifactId>fluxion-tools</artifactId>
    <version>1.0-alpha</version>
  </parent>

  <modelVersion>4.0.0</modelVersion>
  <groupId>net.sourceforge.fluxion.ajax</groupId>
  <artifactId>fluxion-ajax-js</artifactId>
  <name>Fluxion AJAX Javascript Library</name>
  <packaging>jar</packaging>
  <version>1.0-alpha</version>
  <url>http://fluxion.sourceforge.net/fluxion-ajax-js</url>

  <!-- profile for building with java 5 -->
  <profiles>
    <profile>
      <id>java-5</id>
      <build>
        <plugins>
          <plugin>
            <artifactId>maven-compiler-plugin</artifactId>
            <configuration>
              <source>1.5</source>
              <target>1.5</target>
              <verbose>true</verbose>
              <fork>true</fork>
              <executable>${JAVA_5_HOME}/bin/javac</executable>
              <compilerVersion>1.5</compilerVersion>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
  </profiles>

</project>
